<!-- Offscreen container for print-resume HTML (must be rendered for html2pdf) -->
<div
	id="print-resume-container"
	style="position:fixed;left:-10000px;top:0;width:800px;background:#ffffff;opacity:0;pointer-events:none;z-index:-1;"
>
</div>

<script>
	// @ts-nocheck
	// Resume navigation and download functionality
	const setupResumeInteractions = () => {
		const navLinks = document.querySelectorAll(".resume-nav-link");
		const sections = document.querySelectorAll(".resume-section");

		// Function to update active nav link
		/** @param {string} targetId */
		function updateActiveNavLink(targetId) {
			navLinks.forEach((link) => {
				link.classList.remove(
					"bg-orange-50",
					"dark:bg-orange-900/20",
					"text-orange-600",
					"dark:text-orange-400"
				);
				link.classList.add("text-gray-700", "dark:text-gray-300");
			});

			const activeLink = document.querySelector(`[data-section="${targetId}"]`);
			if (activeLink) {
				activeLink.classList.remove("text-gray-700", "dark:text-gray-300");
				activeLink.classList.add(
					"bg-orange-50",
					"dark:bg-orange-900/20",
					"text-orange-600",
					"dark:text-orange-400"
				);
			}
		}

		// Function to scroll to section with proper offset
		/** @param {string} sectionId */
		function scrollToSection(sectionId) {
			const section = document.getElementById(sectionId);
			if (section) {
				// Calculate offset to account for sticky sidebar and spacing
				const yOffset = -80;
				const y =
					section.getBoundingClientRect().top + window.pageYOffset + yOffset;

				window.scrollTo({
					top: y,
					behavior: "smooth",
				});
			}
		}

		// Add click event listeners to nav links
		navLinks.forEach((link) => {
			link.addEventListener("click", (e) => {
				e.preventDefault();
				const targetId = link.getAttribute("data-section");
				if (targetId) {
					scrollToSection(targetId);
					updateActiveNavLink(targetId);
				}
			});
		});

		// Intersection Observer to update active nav link based on scroll position
		const observerOptions = {
			root: null,
			rootMargin: "-10% 0px -60% 0px", // Improved margins for better section detection
			threshold: [0, 0.1, 0.5, 1],
		};

		const observer = new IntersectionObserver((entries) => {
			// Find the entry with the highest intersection ratio
			let maxEntry = null;
			let maxRatio = 0;

			entries.forEach((entry) => {
				if (entry.isIntersecting && entry.intersectionRatio > maxRatio) {
					maxEntry = entry;
					maxRatio = entry.intersectionRatio;
				}
			});

			if (maxEntry) {
				const sectionId = maxEntry.target.id;
				updateActiveNavLink(sectionId);
			}
		}, observerOptions);

		// Observe all sections
		sections.forEach((section) => {
			observer.observe(section);
		});

		// Set initial active state
		if (sections.length > 0) {
			updateActiveNavLink(sections[0].id);
		}

		// Resume download functionality
		const downloadBtn = document.querySelector("[data-download-resume]");
		if (downloadBtn) {
			downloadBtn.addEventListener("click", async () => {
				try {
					const container = document.getElementById("print-resume-container");
					if (!container) {
						console.error("Container not found");
						return;
					}
					// Create an offscreen iframe to load the printable page with its styles
					container.style.position = "fixed";
					container.style.left = "-10000px";
					container.style.top = "0";
					container.style.width = "800px";
					container.style.background = "#ffffff";
					container.style.opacity = "0";
					container.style.pointerEvents = "none";
					container.style.zIndex = "-1";

					const iframe = document.createElement("iframe");
					iframe.src = "/print-resume";
					iframe.style.width = "800px";
					iframe.style.height = "1200px"; // enough to render full page; html2pdf will paginate
					iframe.style.border = "0";
					container.appendChild(iframe);

					await new Promise((resolve, reject) => {
						iframe.onload = resolve;
						iframe.onerror = reject;
					});

					const doc = iframe.contentDocument || iframe.contentWindow?.document;
					if (!doc) {
						throw new Error("Unable to access iframe document");
					}
					// Force a white background to avoid dark-mode inversions
					doc.body.style.background = "#ffffff";

					// Wait for fonts and images to be fully loaded for accurate rendering
					if (doc.fonts && typeof doc.fonts.ready?.then === "function") {
						try {
							await doc.fonts.ready;
						} catch {}
					}
					const imgs = Array.from(doc.images || []);
					await Promise.all(
						imgs.map((img) =>
							img.complete && img.naturalWidth !== 0
								? Promise.resolve()
								: new Promise((resolve) => {
										img.addEventListener("load", resolve, { once: true });
										img.addEventListener("error", resolve, { once: true });
									})
						)
					);
					// One frame to settle layout
					await new Promise((r) =>
						requestAnimationFrame(() => requestAnimationFrame(r))
					);

					// Trigger native print of the iframe for pixel-perfect PDF
					const win = iframe.contentWindow;
					if (!win) throw new Error("No iframe window");
					const cleanup = () => {
						try {
							container.removeChild(iframe);
						} catch {}
					};
					// Some browsers support onafterprint; also add a timeout fallback
					win.focus();
					const afterPrint = () => {
						win.removeEventListener?.("afterprint", afterPrint);
						cleanup();
					};
					// Attach afterprint if possible
					try {
						win.addEventListener?.("afterprint", afterPrint);
					} catch {}
					win.print();
					// Fallback cleanup in case afterprint doesn't fire
					setTimeout(cleanup, 5000);
				} catch (error) {
					console.error("Error generating PDF:", error);
					alert("Failed to generate PDF. Please try again.");
				}
			});
		}
	};

	if (document.readyState === "loading") {
		document.addEventListener("DOMContentLoaded", setupResumeInteractions);
	} else {
		setupResumeInteractions();
	}
</script>
